<%- include('../../shared/header',) %>
<div class="container">

    <h1>Show Class</h1>

   
    <form action="class"
        method="POST"
    >
    <div class="form-row">
        <div class="form-group col-md-6" contenteditable="false">
          <label for="street">Subject</label>
          <input 
          disabled="true"
          type="text" 
                 class="form-control" 
                 id="street" 
                 name="street" 
                 value="Phisyc"
                 placeholder="Phisyc">
        </div>
        <div class="form-group col-md-6">
          <label for="teacher">Date</label>
          <input type="text" disabled="true" class="form-control" id="teacher" name="teacher" placeholder="2/21/2020">
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="subjectID">City</label>
          <input type="text" disabled="true" class="form-control" 
            id="subjectID" 
            placeholder="Quito"
            name="subject"
            
            >
        </div>
        <div class="form-group col-md-6">
          <label for="scheduleID">Sector</label>
          <input type="text" disabled="true" class="form-control" id="scheduleID" name="Schedule" placeholder="Chimbacalle">
        </div>
        
      </div>     
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="subjectID">Schedule</label>
          <input type="text" disabled="true" class="form-control" 
            id="subjectID" 
            placeholder="7:00 - 8:00"
            name="subject"
            
            >
        </div>
        <div class="form-group col-md-6">
          <label for="scheduleID">Precie</label>
          <input type="text" disabled="true" class="form-control" id="scheduleID" name="Schedule" placeholder="5,60">
        </div>
        
      </div> 
    </form>
</div>

<%- include('../../shared/foot-page',) %>
